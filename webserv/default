http {
	index something;
	limit_except GET POST DELETE;

	server {
		server_name localhost default_server;
		listen 8082;
		client_body_size 42m;
		root ~/default/;
		error_page 404 /404.html;
		index index.html test.html error.html;
		autoindex on;

		location = /example {
			root www/html/;
			index sonkang.html;
			return 301 dcho;
		}
		
		location = /form.php {\
			root value;
			index value;
		}
	}

	server {
		server_name page2.com;
		listen 8083;
		client_body_size 42m;
		root /;
		index index.html test.html error.html;
		error_page 404 /404.html;
		autoindex on;

		location = /example {
			#location block 안에 무슨 디렉티브가 오는지 정하자!
			root value;
			index value1.html;
			return 301 http://page1.com;
		}
	}

	// server {
	// 	server_name page1.com;
	// 	listen 8081;
	// 	client_body_size 42m;
	// 	root /;
	// 	index index.html test.html error.html;
	// 	error_page 404 /404.html;
	// 	autoindex on;

	// 	location = /uri {
	// 		#location block 안에 무슨 디렉티브가 오는지 정하자!
	// 		root value;
	// 		index value;
	// 		return 301 http://uri;
	// 	}
	// }
}
