http {
	index something;
	limit_except GET POST DELETE;

	server {
		server_name _ default_server;
		listen 8080;
		client_body_size 42m;
		root /;
		index index.html test.html error.html;
		autoindex on;

		location = / {
			root /;
			index sonkang.html;
			redirect dcho;
			error_page 301 http://uri;
		}

		location ~ \.php$ {
			# '$' -> cgi resource process
			root value;
			index value;
		}

		location = / {
			error_page 404 /404.html;
		}
	}

	server {
		server_name page2.com;
		listen 8080;
		client_body_size 42m;
		root /;
		index index.html test.html error.html;
		autoindex on;

		location = / uri {
			#location block 안에 무슨 디렉티브가 오는지 정하자!
			root value;
			index value;
			return 301 http://uri;
		}

		location / {
			error_page 404 /404.html;
		}
	}
}
